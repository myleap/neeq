<view style="background:url(https://www.myleap.cn/neeqAdmin/public/images/indexbg.jpeg) no-repeat;width:{{winWidth}}px;height:{{winHeight}}px;background-size:100% 100%;">
  <navigator class="" style="padding:100rpx 100rpx;" url="./company/search?type=detail">
    <view class="weui-search-bar__form">
      <view class="weui-search-bar__box">
        <icon class="weui-icon-search_in-box" type="search" size="14"></icon>
        <input type="text" class="weui-search-bar__input" confirm-type="搜索" placeholder="搜索" bindinput="toSearch" />
      </view>
      <label class="weui-search-bar__label">
        <icon class="weui-icon-search" type="search" size="14"></icon>
        <view class="weui-search-bar__text">搜索</view>
      </label>
    </view>
  </navigator>
